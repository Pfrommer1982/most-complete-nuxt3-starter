#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/nuxt@3.12.4_eslint@9.8.0_rollup@4.20.0_sass@1.77.8_typescript@5.5.4_vite@5.3.5/node_modules/nuxt/bin/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/nuxt@3.12.4_eslint@9.8.0_rollup@4.20.0_sass@1.77.8_typescript@5.5.4_vite@5.3.5/node_modules/nuxt/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/nuxt@3.12.4_eslint@9.8.0_rollup@4.20.0_sass@1.77.8_typescript@5.5.4_vite@5.3.5/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/nuxt@3.12.4_eslint@9.8.0_rollup@4.20.0_sass@1.77.8_typescript@5.5.4_vite@5.3.5/node_modules/nuxt/bin/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/nuxt@3.12.4_eslint@9.8.0_rollup@4.20.0_sass@1.77.8_typescript@5.5.4_vite@5.3.5/node_modules/nuxt/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/nuxt@3.12.4_eslint@9.8.0_rollup@4.20.0_sass@1.77.8_typescript@5.5.4_vite@5.3.5/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../nuxt@3.12.4_eslint@9.8.0_rollup@4.20.0_sass@1.77.8_typescript@5.5.4_vite@5.3.5/node_modules/nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../../../../../../nuxt@3.12.4_eslint@9.8.0_rollup@4.20.0_sass@1.77.8_typescript@5.5.4_vite@5.3.5/node_modules/nuxt/bin/nuxt.mjs" "$@"
fi
