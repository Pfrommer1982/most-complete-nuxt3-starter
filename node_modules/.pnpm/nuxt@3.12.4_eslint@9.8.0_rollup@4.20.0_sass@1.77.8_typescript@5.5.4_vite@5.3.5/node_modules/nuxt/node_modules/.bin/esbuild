#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/esbuild@0.23.0/node_modules/esbuild/bin/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/esbuild@0.23.0/node_modules/esbuild/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/esbuild@0.23.0/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/esbuild@0.23.0/node_modules/esbuild/bin/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/esbuild@0.23.0/node_modules/esbuild/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/esbuild@0.23.0/node_modules:/Users/christoph/Desktop/try-out 2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../../../../esbuild@0.23.0/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
